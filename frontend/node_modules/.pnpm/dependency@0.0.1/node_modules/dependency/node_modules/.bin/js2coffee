#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Proyectos/LADETEC/frontend/node_modules/.pnpm/js2coffee@2.2.0/node_modules/js2coffee/bin/node_modules:/mnt/d/Proyectos/LADETEC/frontend/node_modules/.pnpm/js2coffee@2.2.0/node_modules/js2coffee/node_modules:/mnt/d/Proyectos/LADETEC/frontend/node_modules/.pnpm/js2coffee@2.2.0/node_modules:/mnt/d/Proyectos/LADETEC/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Proyectos/LADETEC/frontend/node_modules/.pnpm/js2coffee@2.2.0/node_modules/js2coffee/bin/node_modules:/mnt/d/Proyectos/LADETEC/frontend/node_modules/.pnpm/js2coffee@2.2.0/node_modules/js2coffee/node_modules:/mnt/d/Proyectos/LADETEC/frontend/node_modules/.pnpm/js2coffee@2.2.0/node_modules:/mnt/d/Proyectos/LADETEC/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../js2coffee@2.2.0/node_modules/js2coffee/bin/js2coffee" "$@"
else
  exec node  "$basedir/../../../../../js2coffee@2.2.0/node_modules/js2coffee/bin/js2coffee" "$@"
fi
